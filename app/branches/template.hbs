<div class="yg-row">
  <div class="yg-col-3">
    <ul class="sidebar">
      {{#each model as |blobbity|}}
        <li class="sidebar__item">
          {{blobbity.name}} -
          {{blobbity.location}}
        </li>
      {{/each}}

      <li class="sidebar__item">
        {{#link-to "branches.new"}}
          {{fa-icon "plus"}}
        {{/link-to}}
      </li>
    </ul>
  </div>

  <div class="yg-col-9">
    {{outlet}}
  </div>
</div>
